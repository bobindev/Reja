            <div class="midd_frame">
                <div class="midd_frame_1">
                    <div class="midd_frame_1-1">
                        <div class="midd_frame_1-1_1">
                            <h1>
                                <%=user.work_place%>
                            </h1>
                        </div>
                        <div class="midd_frame_1-1_2">
